    <PostBuildEvent>REM ================== Post-Build: waabe_navi_mcpserver ==================
REM Bundle-Zielpfade (anpassbar)
set NW_VER=v23
set BUNDLE=%25APPDATA%25\Autodesk\ApplicationPlugins\waabe_navi_mcp.bundle\
set CONTENT=%25BUNDLE%25Contents\%25NW_VER%25\
set SETTINGS=%25BUNDLE%25Settings\

REM Verzeichnisse sicherstellen
if not exist "%25CONTENT%25"  mkdir "%25CONTENT%25"
if not exist "%25SETTINGS%25" mkdir "%25SETTINGS%25"

REM ------------------ Manifest kopieren ------------------
REM Hinweis: Wir legen das mcp_manifest.json wie bisher in %25CONTENT%25 ab
if exist "$(ProjectDir)mcp_manifest.json" (
    xcopy "$(ProjectDir)mcp_manifest.json" "%25CONTENT%25\" /Y /Q
) else (
    echo [WARN] Manifest mcp_manifest.json nicht gefunden.
)

REM ------------------ Haupt-DLL kopieren ------------------
REM Kopiert die gebaute waabe_navi_mcp_server.dll in das Bundle
xcopy "$(TargetPath)" "%25CONTENT%25\" /Y /Q



</PostBuildEvent>
